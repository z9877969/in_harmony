{
  "name": "in_harmony",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "next lint . --ext js,jsx --report-unused-disable-directives --max-warnings 0 --fix"
  },
  "dependencies": {
    "next": "14.2.22",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "eslint": "^8.57.1",
    "eslint-config-next": "^14.2.22",
    "eslint-plugin-react-refresh": "^0.4.18",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "prettier": "^3.4.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --fix",
    "src/**/*.{json,css,scss,md}": [
      "prettier --write"
    ]
  }
}